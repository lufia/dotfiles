[user]
	name = lufia
	email = lufia@lufia.org
[core]
	excludesfile = ~/lib/git/gitignore_global
	precomposeunicode = true
[push]
	default = simple
[color]
	diff = auto
	ui = auto
[init]
	templatedir = ~/lib/git/template
[credential]
	helper = factotum
[http]
	cookiefile = ~/lib/git/gitcookies
[credential "https://source.developers.google.com"]
	helper = gcloud.sh
[merge]
	conflictstyle = diff3
[pull]
	rebase = false
